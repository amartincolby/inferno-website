    "jwt-simple": "^0.5.6",
    "koa": "^2.13.1",
    "koa-better-body": "^3.3.9",
    "koa-compress": "^5.0.1",
    "koa-convert": "^2.0.0",
    "koa-favicon": "^2.1.0",
    "koa-mount": "^4.0.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "markdown-loader": "^6.0.0",
    "mini-css-extract-plugin": "^1.4.0",
    "mz": "^2.7.0",
    "node-sass": "5.0.0",
    "prismjs": "^1.23.0",
    "sass-loader": "11.0.1",
    "serve-favicon": "2.5.0",
